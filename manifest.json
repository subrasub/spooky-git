{
	"name": "spooky-git",
	"description": "Halloween themed git-contributions!",
	"browser_action": {
        "default_icon": {
            "16": "icons/16.png",
            "32": "icons/32.png",
            "48": "icons/48.png",
            "64": "icons/64.png",
            "128": "icons/128.png",
            "1024": "icons/1024.png"
        },
        "default_title": "spooky-git",
        "default_popup": "popup.html"
        },
  "icons": {
      "16": "icons/16.png",
      "32": "icons/32.png",
      "48": "icons/48.png",
      "64": "icons/64.png",
      "128": "icons/128.png",
      "1024": "icons/1024.png"
    },
	"manifest_version": 2,
	"version": "1.0.0",
	"author": "Subramanian Subramanian",
	"content_scripts": [{
		"matches": ["*://github.com/*"],
		"js": ["halloween.js"],
		"run_at": "document_end",
		"all_frames": true
	}]
}
